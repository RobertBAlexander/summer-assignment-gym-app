

<table class="ui center aligned raised celled table segment">
    <thead>
    <tr>

        <th>Weight</th>
        <th>Chest</th>
        <th>Thigh</th>
        <th>Upper Arm</th>
        <th>Waist</th>
        <th>Hips</th>
        <th>Comment</th>
        <th>Trend</th>
        <th>Remove Assessment</th>
    </tr>
    </thead>

    <tbody>
    <div class="ui raised list">

    {{#each user.assessments}}

        <tr>

            <td> {{weight}} </td>
            <td> {{chest}} </td>
            <td> {{thigh}} </td>
            <td> {{upperArm}} </td>
            <td> {{waist}} </td>
            <td> {{hips}} </td>
            <td> {{comment}} </td>
            <td> {{trend}} </td>

            <td><a href="/dashboard/deleteassessment/{{id}}" class="ui big grey
               icon button">
                <i class="icon trash"></i></a></td>
        </tr>
    {{/each}}
    </div>
    </tbody>
</table>