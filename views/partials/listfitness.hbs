<section class="ui raised segment">
    {{#each fitnessArray}} <!--supposed to get the stuff from EVERY assessment in a list order.-->
        <div class="ui column">
            <table class="ui center aligned raised celled table segment">

                <thead>
                <tr>
                    <th>Name of Fitness Program</th>
                    <th>Session 1</th>
                    <th>Session 2</th>
                    <th>Session 3</th>
                    <th>Session 4</th>
                    <th>Session 5</th>

                </tr>
                </thead>

                <tbody>
                <div class="ui raised list">
                    <tr>
                        <td> {{programName}} hello</td>
                        {{#each sessions}}
                            <td> {{sessionName}}, a {{sessionType}} session {{#if classId}}<a href="memberfitness/classAttend/{{classId}}" class="ui big green
               icon button">
                                <i class="icon sign in"></i></a>{{/if}} </td>
                        {{/each}}
                </div>
                </tbody>
            </table>
            <br><br>
        </div>
    {{/each}}
</section>
