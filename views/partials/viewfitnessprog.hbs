
<section class="ui raised segment">
    {{#each trainer.fitnessprog}} <!--supposed to get the stuff from EVERY assessment in a list order.-->
        <div class="ui column">
            <table class="ui center aligned raised celled table segment">

                <thead>
                <tr>
                    <th>Name of Fitness Program</th>
                    <th>Session 1</th>
                    <th>Session 2</th>
                    <th>Session 3</th>
                    <th>Session 4</th>
                    <th>Session 5</th>
                    <th>Remove Fitness Programme</th>
                </tr>
                </thead>

                <tbody>
                <div class="ui raised list">
                    <tr>
                        <td> {{fitnessprog.programname}} </td>
                        <td> {{fitnessprog.sessionone}} </td>
                        <td> {{fitnessprog.sessiontwo}} </td>
                        <td> {{fitnessprog.sessionthree}} </td>
                        <td> {{fitnessprog.sessionfour}} </td>
                        <td> {{fitnessprog.sessionfive}} </td>
                        <td><a href="/trainer/dashboard/viewmember/{{trainer.id}}/deletefitnessprog/{{fitnessprog.id}}" class="ui big grey
               icon button">
                            <i class="icon trash"></i></a></td>
                    </tr>
                </div>
                </tbody>
            </table>
            <br><br>
        </div>
    {{/each}}
</section>
