
<section class="ui raised segment">
    {{#each fitnessArray}} <!--supposed to get the stuff from EVERY assessment in a list order.-->
        <div class="ui column">
            <table class="ui center aligned raised celled table segment">

                <thead>
                <tr>
                    <th>Name of Fitness Program</th>
                    <th>Session 1</th>
                    <th>Session 2</th>
                    <th>Session 3</th>
                    <th>Session 4</th>
                    <th>Session 5</th>
                    <th>Edit Fitness Programme</th>
                    <th>Remove Fitness Programme</th>
                </tr>
                </thead>

                <tbody>
                <div class="ui raised list">
                    <tr>
                        <td> {{programName}} </td>
    {{#each sessions}}
                        <td> {{sessionName}}, a {{sessionType}} session </td>
    {{/each}}
                        <td><a href="/fitnesscreation/{{userId}}/fitnessprogdetails/{{programId}}" class="ui big green
               icon button">
                            <i class="icon edit"></i></a></td>
                        <td><a href="/fitnesscreation/{{userId}}/deleteFitnessProg/{{programId}}" class="ui big grey
               icon button">
                            <i class="icon trash"></i></a></td>
                    </tr>
                </div>
                </tbody>
            </table>
            <br><br>
        </div>
    {{/each}}
</section>
